      %h1= mainText
    .col-md-1.col-md-offset-3.col-sm-4.col-sm-offset-2.col-xs-offset-2.col-xs-4.align-center
      / %a{:href => "/storage_items.html"}
      = link_to faq_path do
        .service-box
          .service-icon
            %i.fa.fa-clock-o.fa-3x
          .service-desc
            %h6
              Dashboard
    .col-md-1.col-sm-4.col-xs-4.align-center
      = link_to faq_path do
        .service-box
          .service-icon
            %i.fa.fa-dropbox
          .service-desc
            %h6
              FAQ
